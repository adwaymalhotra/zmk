/ {
  combos {
    compatible = "zmk,combos";
    // top row
    c_tilde {
      timeout-ms = <40>;
      key-positions = <1 2>;
      bindings = <&kp TILDE>;
      layers = <0 1 2 3>;
    };
    c_grave {
      timeout-ms = <40>;
      key-positions = <2 3>;
      bindings = <&kp GRAVE>;
      layers = <0 1 2 3>;
    };
    c_eql {
      timeout-ms = <40>;
      key-positions = <6 7>;
      bindings = <&kp EQUAL>;
      layers = <0 1 2 3>;
    };
    c_plus {
      timeout-ms = <40>;
      key-positions = <7 8>;
      bindings = <&kp PLUS>;
      layers = <0 1 2 3>;
    };
    // bottom row {([ ])}
    c_lbrc {
      timeout-ms = <40>;
      key-positions = <22 23>;
      bindings = <&kp LBRC>;
      layers = <0 1 2 3>;
    };
    c_lpar {
      timeout-ms = <40>;
      key-positions = <23 24>;
      bindings = <&kp LPAR>;
      layers = <0 1 2 3>;
    };
    c_lbkt {
      timeout-ms = <40>;
      key-positions = <24 25>;
      bindings = <&kp LBKT>;
      layers = <0 1 2 3>;
    };
    c_rbkt {
      timeout-ms = <40>;
      key-positions = <26 27>;
      bindings = <&kp RBKT>;
      layers = <0 1 2 3>;
    };
    c_rpar {
      timeout-ms = <40>;
      key-positions = <27 28>;
      bindings = <&kp RPAR>;
      layers = <0 1 2 3>;
    };
    c_rbrc {
      timeout-ms = <40>;
      key-positions = <28 29>;
      bindings = <&kp RBRC>;
      layers = <0 1 2 3>;
    };
  };
};
